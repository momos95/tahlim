{% extends "TKCoreBundle::layout.html.twig" %}

{% block title %}
    {{ parent() }} - Cours PDF
{% endblock %}

{% block body %}

    <div class="container-fluid">

        <object type="application/pdf" width="100%" height="100%" style="position: relative; display: inline-block; margin-top: 30px; height: 800px; background: transparent">
            <param name="src" value="{{ asset(url) }}" />

            <p>It appears your Web browser is not configured to display PDF files.
                No worries, just <a href="{{ asset(url) }}">click here to download the PDF file.</a>
            </p>

        </object>
    </div>



{% endblock %}